<script>
	/*Menu-toggle*/
	$(document).ready(function() {
		$("#selectChangable").val("All States");
	    $("#menu-toggle").click(function() {
			if($("#RightPage").attr("class")=="col-xs-10")
			{
			$("#Sidebar").css("display","none");
			$("#RightPage").attr("class","col-xs-12");
			}
			else
			{
			$("#Sidebar").css("display","inherit");
			$("#RightPage").attr("class","col-xs-10");
			}
    });
		$("#btn01").click(function(){
			$('#myCarousel').carousel("prev");
			});
	
});
</script>
